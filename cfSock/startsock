#  Script to load cfSockSer, with all necessary utilities
#  usage:
#   ->  < /adaqfs/halla/apar/kent/devices/cfSock/startsock
# 
#
#  load time board routines 
#       (must be done first, to define globals used elsewhere)
#
ld < /adaqfs/halla/apar/kent/devices/timebrd/HAPTB_util.o
ld < /adaqfs/halla/apar/kent/devices/timebrd/HAPTB_config.o
initHAPTB
#
#  load adc routines
#
ld < /adaqfs/halla/apar/kent/devices/adc/HAPADC.o
initHAPADC
#
#  load bmw routines
#
ld < /adaqfs/halla/apar/kent/devices/bmw/bmwClient.o
ld < /adaqfs/halla/apar/kent/devices/bmw/bmw_config.o
#
#  add route to bmw Server IOC
#
routeAdd "129.57.236.67","129.57.164.1"
#
#  load cfSock server
#
ld < /adaqfs/halla/apar/kent/devices/cfSock/cfSockSer.o
#
#  to start, use command:
#  sp cfSockSer
#